thelist = ["Banana","Ahmad","Apple","Orange"]

for u in thelist:
    print(u)

print("------------------------------------Change Item Value -----------------------------------------------")


thelist = ["Banana","Ahmad","Apple","Orange"]
thelist[1]="saad"
print(thelist)


print("------------------------------------Delete Item Value -----------------------------------------------")
thelist = ["Banana","Ahmad","Apple","Orange"]
del thelist[1]
print(thelist)
